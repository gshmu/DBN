<?xml version="1.0" encoding="UTF-8"?>
<!--<!DOCTYPE element-defs SYSTEM "/../../../common/definition/language-parser-elements.dtd">-->
<element-defs language="SQLITE-SQL" version="6.0">
    <element-def attributes="ROOT, STRUCTURE" description="SQL block" icon="FILE_BLOCK_SQL" id="sql_block">
        <iteration id="000000000">
            <element ref-id="sql_statement" />
        </iteration>
    </element-def>

    <element-def attributes="STRUCTURE, EXECUTABLE, STATEMENT, SCOPE_ISOLATION, GENERIC" description="SQL statement" formatting-indent="ABSOLUTE_NONE" id="sql_statement">
        <one-of id="000000001">
            <element ref-id="alter_statement" />
            <element ref-id="commit_statement" />
            <element ref-id="create_statement" />
            <element ref-id="delete_statement" />
            <element ref-id="drop_statement" />
            <element ref-id="explain_statement" />
            <element ref-id="grant_statement" />
            <element ref-id="insert_statement" />
            <element ref-id="merge_statement" />
            <element ref-id="rollback_statement" />
            <element ref-id="rename_statement" />
            <element ref-id="select_statement" />
            <element ref-id="truncate_statement" />
            <element ref-id="update_statement" />
        </one-of>
        <element optional="true" ref-id="end" />
    </element-def>


    <!-- ========= EXPRESSIONS ========= -->
    <element-def description="Expression" id="expr">
        <one-of id="000000395">
            <element ref-id="compound_expression" />
            <element ref-id="case_expression" />
            <element ref-id="function_expression" />
            <element ref-id="simple_expression" />
            <exec-variable id="000000396" />
        </one-of>
    </element-def>
    <element-def description="Simple expression" id="simple_expression">
        <one-of id="000000397" sortable="true">
            <qualified-identifier id="000000398" separator="CHR_DOT">
                <variant>
                    <object-ref id="000000399" local="true" optional="true" type="SCHEMA" />
                    <object-ref id="000000400" local="true" optional="true" type="DATASET" />
                    <object-ref id="000000401" type="COLUMN" />
                </variant>
                <variant>
                    <object-ref id="000000402" local="true" optional="true" type="SCHEMA" />
                    <object-ref id="000000403" local="true" optional="true" type="DATASET" />
                    <token id="000000404" type-id="CHR_STAR" />
                </variant>
                <variant>
                    <alias-ref id="000000405" type="DATASET" />
                    <object-ref id="000000406" type="COLUMN" />
                </variant>
                <variant>
                    <alias-ref id="000000407" type="DATASET" />
                    <alias-ref id="000000408" type="COLUMN" />
                </variant>
                <variant>
                    <alias-ref id="000000411" type="DATASET" />
                    <token id="000000412" type-id="CHR_STAR" />
                </variant>
            </qualified-identifier>
            <token id="000000413" type-id="CHR_STAR" />
            <token id="000000414" original-name="text" type-id="STRING" />
            <element ref-id="number" />
            <token id="000000415" type-id="KW_NULL" />
        </one-of>
    </element-def>
    <element-def description="Compound expression" id="compound_expression">
        <one-of id="000000416">
            <sequence id="000000417">
                <element ref-id="expr" />
                <one-of id="000000418">
                    <token id="000000419" type-id="CHR_STAR" />
                    <token id="000000420" type-id="CHR_SLASH" />
                    <token id="000000421" type-id="CHR_PLUS" />
                    <token id="000000422" type-id="CHR_MINUS" />
                    <element ref-id="concatenation_operator" />
                </one-of>
                <element exit="true" ref-id="expr" />
            </sequence>
            <sequence id="000000423">
                <one-of id="000000424">
                    <token id="000000425" type-id="CHR_PLUS" />
                    <token id="000000426" type-id="CHR_MINUS" />
                </one-of>
                <element ref-id="expr" />
            </sequence>
            <wrapper id="000000427" template="PARENTHESES">
                <one-of id="000000428">
                    <element ref-id="expr" />
                    <element ref-id="subquery" />
                </one-of>
            </wrapper>
            <element ref-id="expr" />
        </one-of>
    </element-def>
    <element-def description="CASE expression" id="case_expression">
        <token id="000000429" type-id="KW_CASE" />
        <one-of id="000000430" sortable="true">
            <sequence id="000000431">
                <element ref-id="expr" />
                <iteration exit="true" id="000000432">
                    <sequence id="000000433">
                        <token id="000000434" type-id="KW_WHEN" />
                        <element ref-id="expr" />
                        <token id="000000435" type-id="KW_THEN" />
                        <element ref-id="expr" />
                    </sequence>
                </iteration>
            </sequence>
            <iteration id="000000436">
                <sequence id="000000437">
                    <token id="000000438" type-id="KW_WHEN" />
                    <element ref-id="condition" />
                    <token id="000000439" type-id="KW_THEN" />
                    <element ref-id="expr" />
                </sequence>
            </iteration>
        </one-of>
        <sequence id="000000440" optional="true">
            <token id="000000441" type-id="KW_ELSE" />
            <element ref-id="expr" />
        </sequence>
        <token id="000000442" type-id="KW_END" />
    </element-def>
    <element-def description="Expression list" id="expression_list">
        <one-of id="000000443" sortable="true">
            <iteration id="000000444" separator="CHR_COMMA">
                <element ref-id="expr" />
            </iteration>
            <wrapper id="000000445" template="PARENTHESES">
                <iteration id="000000446" separator="CHR_COMMA">
                    <element ref-id="expr" />
                </iteration>
            </wrapper>
        </one-of>
    </element-def>

    <!-- ========= CONDITIONS ========= -->
    <element-def description="Condition" id="condition">
        <one-of id="000000447">
            <!--element ref-id="logical_conditions"/-->
            <iteration formatting-indent="NORMAL" id="000000448" original-name="compound_condition" separator="KW_AND, KW_OR">
                <element ref-id="condition" />
            </iteration>
            <element ref-id="exists_condition" />
            <sequence id="000000449">
                <token id="000000450" type-id="CHR_LEFT_PARENTHESIS" />
                <one-of id="000000451">
                    <sequence id="000000452">
                        <element exit="true" ref-id="condition" />
                        <token id="000000453" type-id="CHR_RIGHT_PARENTHESIS" />
                    </sequence>
                    <sequence id="000000454">
                        <iteration id="000000455" separator="CHR_COMMA">
                            <element ref-id="expr" />
                        </iteration>
                        <token id="000000456" type-id="CHR_RIGHT_PARENTHESIS" />
                        <one-of id="000000457">
                            <sequence id="000000458">
                                <element ref-id="equality_operator" />
                                <wrapper id="000000459" template="PARENTHESES">
                                    <element ref-id="subquery" />
                                </wrapper>
                            </sequence>
                            <sequence id="000000460">
                                <token id="000000461" optional="true" type-id="KW_NOT" />
                                <token exit="true" id="000000462" type-id="KW_IN" />
                                <wrapper id="000000463" template="PARENTHESES">
                                    <one-of id="000000464" sortable="true">
                                        <element ref-id="expression_list" />
                                        <element ref-id="subquery" />
                                    </one-of>
                                </wrapper>
                            </sequence>
                        </one-of>
                    </sequence>
                    <sequence id="000000465"> <!-- custom -->
                        <element exit="true" ref-id="subquery" />
                        <token id="000000466" type-id="CHR_RIGHT_PARENTHESIS" />
                        <one-of id="000000467">
                            <element ref-id="equality_operator" />
                            <element ref-id="greater_less_operator" />
                        </one-of>
                        <element ref-id="expr" />
                    </sequence>
                </one-of>
            </sequence>
            <sequence id="000000468">
                <element ref-id="expr" />
                <one-of id="000000469">
                    <sequence id="000000470">
                        <token id="000000471" optional="true" type-id="KW_NOT" />
                        <one-of id="000000472">
                            <sequence id="000000473">
                                <token id="000000474" type-id="KW_BETWEEN" />
                                <element ref-id="expr" />
                                <token id="000000475" type-id="KW_AND" />
                                <element ref-id="expr" />
                            </sequence>
                            <sequence id="000000476">
                                <token id="000000477" type-id="KW_LIKE" />
                                <element ref-id="expr" />
                            </sequence>
                            <sequence id="000000478">
                                <token id="000000479" type-id="KW_IN" />
                                <wrapper id="000000480" template="PARENTHESES">
                                    <one-of id="000000481" sortable="true">
                                        <element ref-id="expression_list" />
                                        <element ref-id="subquery" />
                                    </one-of>
                                </wrapper>
                            </sequence>
                        </one-of>
                    </sequence>
                    <sequence id="000000482">
                        <token id="000000483" type-id="KW_IS" />
                        <token id="000000484" optional="true" type-id="KW_NOT" />
                        <token id="000000485" type-id="KW_NULL" />
                    </sequence>
                    <sequence id="000000486">
                        <one-of exit="true" id="000000487">
                            <element ref-id="equality_operator" />
                            <element ref-id="greater_less_operator" />
                        </one-of>
                        <element ref-id="expr" />
                    </sequence>
                </one-of>
            </sequence>
            <sequence id="000000488">
                <token id="000000489" type-id="KW_NOT" />
                <element ref-id="condition" />
            </sequence>
        </one-of>
    </element-def>
    <element-def description="EXISTS condition" id="exists_condition">
        <token id="000000490" type-id="KW_EXISTS" />
        <wrapper id="000000491" template="PARENTHESES">
            <element ref-id="subquery" />
        </wrapper>
    </element-def>

    <!-- ========= FUNCTIONS ========= -->
    <element-def description="Declared function expression" id="declared_function_expression"> <!-- CUSTOM-->
        <qualified-identifier id="000000492" separator="CHR_DOT">
            <variant>
                <object-ref id="000000493" optional="true" type="SCHEMA" />
                <object-ref id="000000494" type="FUNCTION" />
            </variant>
        </qualified-identifier>
        <wrapper exit="true" id="000000495" template="PARENTHESES" attributes="METHOD_PARAMETER_HANDLER">
            <iteration id="000000496" optional="true" separator="CHR_COMMA">
                <element ref-id="expr" />
            </iteration>
        </wrapper>
    </element-def>
    <element-def description="Function expression" id="function_expression">
        <sequence id="000000498">
            <one-of id="000000499">
                <token id="000000522" type-id="FN_AVG" />
                <token id="000000522" type-id="FN_ABS" />
                <token id="000000522" type-id="FN_CHANGES" />
                <token id="000000522" type-id="FN_CHAR" />
                <token id="000000522" type-id="FN_COALESCE" />
                <token id="000000522" type-id="FN_COUNT" />
                <token id="000000522" type-id="FN_GROUP_CONCAT" />
                <token id="000000522" type-id="FN_HEX" />
                <token id="000000522" type-id="FN_IFNULL" />
                <token id="000000522" type-id="FN_INSTR" />
                <token id="000000522" type-id="FN_JSON" />
                <token id="000000522" type-id="FN_JSON_ARRAY" />
                <token id="000000522" type-id="FN_JSON_ARRAY_LENGTH" />
                <token id="000000522" type-id="FN_JSON_EXTRACT" />
                <token id="000000522" type-id="FN_JSON_INSERT" />
                <token id="000000522" type-id="FN_JSON_OBJECT" />
                <token id="000000522" type-id="FN_JSON_REMOVE" />
                <token id="000000522" type-id="FN_JSON_REPLACE" />
                <token id="000000522" type-id="FN_JSON_SET" />
                <token id="000000522" type-id="FN_JSON_TYPE" />
                <token id="000000522" type-id="FN_JSON_VALID" />
                <token id="000000522" type-id="FN_JULIANDAY" />
                <token id="000000522" type-id="FN_LAST_INSERT_ROWID" />
                <token id="000000522" type-id="FN_LENGTH" />
                <token id="000000522" type-id="FN_LIKELIHOOD" />
                <token id="000000522" type-id="FN_LIKELY" />
                <token id="000000522" type-id="FN_LOAD_EXTENSION" />
                <token id="000000522" type-id="FN_LOWER" />
                <token id="000000522" type-id="FN_LTRIM" />
                <token id="000000522" type-id="FN_MAX" />
                <token id="000000522" type-id="FN_MIN" />
                <token id="000000522" type-id="FN_NULLIF" />
                <token id="000000522" type-id="FN_PRINTF" />
                <token id="000000522" type-id="FN_QUOTE" />
                <token id="000000522" type-id="FN_RANDOM" />
                <token id="000000522" type-id="FN_RANDOMBLOB" />
                <token id="000000522" type-id="FN_ROUND" />
                <token id="000000522" type-id="FN_RTRIM" />
                <token id="000000522" type-id="FN_SOUNDEX" />
                <token id="000000522" type-id="FN_SQLITE_COMPILEOPTION_GET" />
                <token id="000000522" type-id="FN_SQLITE_COMPILEOPTION_USED" />
                <token id="000000522" type-id="FN_SQLITE_SOURCE_ID" />
                <token id="000000522" type-id="FN_SQLITE_VERSION" />
                <token id="000000522" type-id="FN_STRFTIME" />
                <token id="000000522" type-id="FN_SUBSTR" />
                <token id="000000522" type-id="FN_SUM" />
                <token id="000000522" type-id="FN_TIME" />
                <token id="000000522" type-id="FN_TOTAL" />
                <token id="000000522" type-id="FN_TOTAL_CHANGES" />
                <token id="000000522" type-id="FN_TRIM" />
                <token id="000000522" type-id="FN_TYPEOF" />
                <token id="000000522" type-id="FN_UNLIKELY" />
                <token id="000000522" type-id="FN_UNICODE" />
                <token id="000000522" type-id="FN_UPPER" />
                <token id="000000522" type-id="FN_ZEROBLOB" />
            </one-of>
            <wrapper id="000000713" template="PARENTHESES">
                <iteration id="000000714" separator="CHR_COMMA" optional="true">
                    <element ref-id="expr" />
                </iteration>
            </wrapper>

        </sequence>
    </element-def>

    <!-- ========= DATA TYPES ========= -->
    <element-def description="Data type" id="data_type">
        <token id="000000893" type-id="DT_BIGINT" />
        <token id="000000893" type-id="DT_BLOB" />
        <token id="000000893" type-id="DT_BOOLEAN" />
        <token id="000000893" type-id="DT_CHARACTER" />
        <token id="000000893" type-id="DT_CLOB" />
        <token id="000000893" type-id="DT_DATE" />
        <token id="000000893" type-id="DT_DATETIME" />
        <token id="000000893" type-id="DT_DECIMAL" />
        <token id="000000893" type-id="DT_DOUBLE" />
        <token id="000000893" type-id="DT_DOUBLE_PRECISION" />
        <token id="000000893" type-id="DT_FLOAT" />
        <token id="000000893" type-id="DT_INT" />
        <token id="000000893" type-id="DT_INT2" />
        <token id="000000893" type-id="DT_INT8" />
        <token id="000000893" type-id="DT_INTEGER" />
        <token id="000000893" type-id="DT_MEDIUMINT" />
        <token id="000000893" type-id="DT_NATIVE_CHARACTER" />
        <token id="000000893" type-id="DT_NCHAR" />
        <token id="000000893" type-id="DT_NUMERIC" />
        <token id="000000893" type-id="DT_NVARCHAR" />
        <token id="000000893" type-id="DT_REAL" />
        <token id="000000893" type-id="DT_SMALLINT" />
        <token id="000000893" type-id="DT_TEXT" />
        <token id="000000893" type-id="DT_TINYINT" />
        <token id="000000893" type-id="DT_UNSIGNED_BIG_INT" />
        <token id="000000893" type-id="DT_VARCHAR" />
        <token id="000000893" type-id="DT_VARYING_CHARACTER" />
    </element-def>

    <!-- ========= OPERATORS ========= -->
    <element-def description="EQUALITY operator" id="equality_operator">
        <one-of id="000000929">
            <sequence id="000000930">
                <token id="000000931" optional="true" type-id="CHR_EXCLAMATION" />
                <token exit="true" id="000000932" type-id="CHR_EQUAL" />
            </sequence>
            <sequence id="000000933">
                <token id="000000934" type-id="CHR_LESS" />
                <token exit="true" id="000000935" type-id="CHR_GREATER" />
            </sequence>
        </one-of>
    </element-def>
    <element-def description="GREATER/LESS operator" id="greater_less_operator">
        <one-of id="000000936">
            <sequence id="000000937">
                <token id="000000938" type-id="CHR_GREATER" />
                <token id="000000939" optional="true" type-id="CHR_EQUAL" />
            </sequence>
            <sequence id="000000940">
                <token id="000000941" type-id="CHR_LESS" />
                <token id="000000942" optional="true" type-id="CHR_EQUAL" />
            </sequence>
        </one-of>
    </element-def>
    <element-def description="CONCATENATION operator" id="concatenation_operator">
        <token id="000000943" type-id="CHR_VERTICAL_BAR" />
        <token id="000000944" type-id="CHR_VERTICAL_BAR" />
    </element-def>

    <!-- ========= MISCELLANEOUS ========= -->
    <element-def description="Number" id="number">
        <one-of id="000000945" optional="true">
            <token id="000000946" type-id="CHR_MINUS" />
            <token id="000000947" type-id="CHR_PLUS" />
        </one-of>
        <one-of id="000000948">
            <token id="000000949" type-id="NUMBER" />
            <token id="000000950" type-id="INTEGER" />
        </one-of>
    </element-def>

    <element-def description="Unknown identifier" id="custom_undefined">
        <token id="000000951" type-id="IDENTIFIER" />
    </element-def>
    <element-def description="Statement end" formatting-spacing-before="NO_SPACE" id="end" truncate-on-execution="true">
        <token id="000000952" type-id="CHR_SEMICOLON" />
    </element-def>
</element-defs>
