<?xml version="1.0" encoding="UTF-8"?>
<!--<!DOCTYPE element-defs SYSTEM "/../../../common/definition/language-parser-elements.dtd">--><element-defs language="SQLITE-SQL" version="6.0">
    <element-def attributes="ROOT, STRUCTURE" description="SQL block" icon="FILE_BLOCK_SQL" id="sql_block">
        <iteration id="000000000">
            <element ref-id="sql_statement" />
        </iteration>
    </element-def>

    <element-def attributes="STRUCTURE, EXECUTABLE, STATEMENT, SCOPE_ISOLATION, GENERIC" description="SQL statement" formatting-indent="ABSOLUTE_NONE" id="sql_statement">
        <one-of id="000000001">
            <element ref-id="alter_statement" />
            <element ref-id="commit_statement" />
            <element ref-id="create_statement" />
            <element ref-id="delete_statement" />
            <element ref-id="drop_statement" />
            <element ref-id="explain_statement" />
            <element ref-id="grant_statement" />
            <element ref-id="insert_statement" />
            <element ref-id="merge_statement" />
            <element ref-id="rollback_statement" />
            <element ref-id="rename_statement" />
            <element ref-id="select_statement" />
            <element ref-id="truncate_statement" />
            <element ref-id="update_statement" />
        </one-of>
        <element optional="true" ref-id="end" />
    </element-def>


    <!-- ========= EXPRESSIONS ========= -->
    <element-def description="Expression" id="expr">
        <one-of id="000000002">
            <element ref-id="compound_expression" />
            <element ref-id="case_expression" />
            <element ref-id="function_expression" />
            <element ref-id="simple_expression" />
            <exec-variable id="000000003" />
        </one-of>
    </element-def>
    <element-def description="Simple expression" id="simple_expression">
        <one-of id="000000004" sortable="true">
            <qualified-identifier id="000000005" separator="CHR_DOT">
                <variant>
                    <object-ref id="000000006" local="true" optional="true" type="SCHEMA" />
                    <object-ref id="000000007" local="true" optional="true" type="DATASET" />
                    <object-ref id="000000008" type="COLUMN" />
                </variant>
                <variant>
                    <object-ref id="000000009" local="true" optional="true" type="SCHEMA" />
                    <object-ref id="000000010" local="true" optional="true" type="DATASET" />
                    <token id="000000011" type-id="CHR_STAR" />
                </variant>
                <variant>
                    <alias-ref id="000000012" type="DATASET" />
                    <object-ref id="000000013" type="COLUMN" />
                </variant>
                <variant>
                    <alias-ref id="000000014" type="DATASET" />
                    <alias-ref id="000000015" type="COLUMN" />
                </variant>
                <variant>
                    <alias-ref id="000000016" type="DATASET" />
                    <token id="000000017" type-id="CHR_STAR" />
                </variant>
            </qualified-identifier>
            <token id="000000018" type-id="CHR_STAR" />
            <token id="000000019" original-name="text" type-id="STRING" />
            <element ref-id="number" />
            <token id="000000020" type-id="KW_NULL" />
        </one-of>
    </element-def>
    <element-def description="Compound expression" id="compound_expression">
        <one-of id="000000021">
            <sequence id="000000022">
                <element ref-id="expr" />
                <one-of id="000000023">
                    <token id="000000024" type-id="CHR_STAR" />
                    <token id="000000025" type-id="CHR_SLASH" />
                    <token id="000000026" type-id="CHR_PLUS" />
                    <token id="000000027" type-id="CHR_MINUS" />
                    <element ref-id="concatenation_operator" />
                </one-of>
                <element exit="true" ref-id="expr" />
            </sequence>
            <sequence id="000000028">
                <one-of id="000000029">
                    <token id="000000030" type-id="CHR_PLUS" />
                    <token id="000000031" type-id="CHR_MINUS" />
                </one-of>
                <element ref-id="expr" />
            </sequence>
            <wrapper id="000000032" template="PARENTHESES">
                <one-of id="000000033">
                    <element ref-id="expr" />
                    <element ref-id="subquery" />
                </one-of>
            </wrapper>
            <element ref-id="expr" />
        </one-of>
    </element-def>
    <element-def description="CASE expression" id="case_expression">
        <token id="000000034" type-id="KW_CASE" />
        <one-of id="000000035" sortable="true">
            <sequence id="000000036">
                <element ref-id="expr" />
                <iteration exit="true" id="000000037">
                    <sequence id="000000038">
                        <token id="000000039" type-id="KW_WHEN" />
                        <element ref-id="expr" />
                        <token id="000000040" type-id="KW_THEN" />
                        <element ref-id="expr" />
                    </sequence>
                </iteration>
            </sequence>
            <iteration id="000000041">
                <sequence id="000000042">
                    <token id="000000043" type-id="KW_WHEN" />
                    <element ref-id="condition" />
                    <token id="000000044" type-id="KW_THEN" />
                    <element ref-id="expr" />
                </sequence>
            </iteration>
        </one-of>
        <sequence id="000000045" optional="true">
            <token id="000000046" type-id="KW_ELSE" />
            <element ref-id="expr" />
        </sequence>
        <token id="000000047" type-id="KW_END" />
    </element-def>
    <element-def description="Expression list" id="expression_list">
        <one-of id="000000048" sortable="true">
            <iteration id="000000049" separator="CHR_COMMA">
                <element ref-id="expr" />
            </iteration>
            <wrapper id="000000050" template="PARENTHESES">
                <iteration id="000000051" separator="CHR_COMMA">
                    <element ref-id="expr" />
                </iteration>
            </wrapper>
        </one-of>
    </element-def>

    <!-- ========= CONDITIONS ========= -->
    <element-def description="Condition" id="condition">
        <one-of id="000000052">
            <!--element ref-id="logical_conditions"/-->
            <iteration formatting-indent="NORMAL" id="000000053" original-name="compound_condition" separator="KW_AND, KW_OR">
                <element ref-id="condition" />
            </iteration>
            <element ref-id="exists_condition" />
            <sequence id="000000054">
                <token id="000000055" type-id="CHR_LEFT_PARENTHESIS" />
                <one-of id="000000056">
                    <sequence id="000000057">
                        <element exit="true" ref-id="condition" />
                        <token id="000000058" type-id="CHR_RIGHT_PARENTHESIS" />
                    </sequence>
                    <sequence id="000000059">
                        <iteration id="000000060" separator="CHR_COMMA">
                            <element ref-id="expr" />
                        </iteration>
                        <token id="000000061" type-id="CHR_RIGHT_PARENTHESIS" />
                        <one-of id="000000062">
                            <sequence id="000000063">
                                <element ref-id="equality_operator" />
                                <wrapper id="000000064" template="PARENTHESES">
                                    <element ref-id="subquery" />
                                </wrapper>
                            </sequence>
                            <sequence id="000000065">
                                <token id="000000066" optional="true" type-id="KW_NOT" />
                                <token exit="true" id="000000067" type-id="KW_IN" />
                                <wrapper id="000000068" template="PARENTHESES">
                                    <one-of id="000000069" sortable="true">
                                        <element ref-id="expression_list" />
                                        <element ref-id="subquery" />
                                    </one-of>
                                </wrapper>
                            </sequence>
                        </one-of>
                    </sequence>
                    <sequence id="000000070"> <!-- custom -->
                        <element exit="true" ref-id="subquery" />
                        <token id="000000071" type-id="CHR_RIGHT_PARENTHESIS" />
                        <one-of id="000000072">
                            <element ref-id="equality_operator" />
                            <element ref-id="greater_less_operator" />
                        </one-of>
                        <element ref-id="expr" />
                    </sequence>
                </one-of>
            </sequence>
            <sequence id="000000073">
                <element ref-id="expr" />
                <one-of id="000000074">
                    <sequence id="000000075">
                        <token id="000000076" optional="true" type-id="KW_NOT" />
                        <one-of id="000000077">
                            <sequence id="000000078">
                                <token id="000000079" type-id="KW_BETWEEN" />
                                <element ref-id="expr" />
                                <token id="000000080" type-id="KW_AND" />
                                <element ref-id="expr" />
                            </sequence>
                            <sequence id="000000081">
                                <token id="000000082" type-id="KW_LIKE" />
                                <element ref-id="expr" />
                            </sequence>
                            <sequence id="000000083">
                                <token id="000000084" type-id="KW_IN" />
                                <wrapper id="000000085" template="PARENTHESES">
                                    <one-of id="000000086" sortable="true">
                                        <element ref-id="expression_list" />
                                        <element ref-id="subquery" />
                                    </one-of>
                                </wrapper>
                            </sequence>
                        </one-of>
                    </sequence>
                    <sequence id="000000087">
                        <token id="000000088" type-id="KW_IS" />
                        <token id="000000089" optional="true" type-id="KW_NOT" />
                        <token id="000000090" type-id="KW_NULL" />
                    </sequence>
                    <sequence id="000000091">
                        <one-of exit="true" id="000000092">
                            <element ref-id="equality_operator" />
                            <element ref-id="greater_less_operator" />
                        </one-of>
                        <element ref-id="expr" />
                    </sequence>
                </one-of>
            </sequence>
            <sequence id="000000093">
                <token id="000000094" type-id="KW_NOT" />
                <element ref-id="condition" />
            </sequence>
        </one-of>
    </element-def>
    <element-def description="EXISTS condition" id="exists_condition">
        <token id="000000095" type-id="KW_EXISTS" />
        <wrapper id="000000096" template="PARENTHESES">
            <element ref-id="subquery" />
        </wrapper>
    </element-def>

    <!-- ========= FUNCTIONS ========= -->
    <element-def description="Declared function expression" id="declared_function_expression"> <!-- CUSTOM-->
        <qualified-identifier id="000000097" separator="CHR_DOT">
            <variant>
                <object-ref id="000000098" optional="true" type="SCHEMA" />
                <object-ref id="000000099" type="FUNCTION" />
            </variant>
        </qualified-identifier>
        <wrapper attributes="METHOD_PARAMETER_HANDLER" exit="true" id="000000100" template="PARENTHESES">
            <iteration id="000000101" optional="true" separator="CHR_COMMA">
                <element ref-id="expr" />
            </iteration>
        </wrapper>
    </element-def>
    <element-def description="Function expression" id="function_expression">
        <sequence id="000000102">
            <one-of id="000000103">
                <token id="000000104" type-id="FN_AVG" />
                <token id="000000105" type-id="FN_ABS" />
                <token id="000000106" type-id="FN_CHANGES" />
                <token id="000000107" type-id="FN_CHAR" />
                <token id="000000108" type-id="FN_COALESCE" />
                <token id="000000109" type-id="FN_COUNT" />
                <token id="000000110" type-id="FN_GROUP_CONCAT" />
                <token id="000000111" type-id="FN_HEX" />
                <token id="000000112" type-id="FN_IFNULL" />
                <token id="000000113" type-id="FN_INSTR" />
                <token id="000000114" type-id="FN_JSON" />
                <token id="000000115" type-id="FN_JSON_ARRAY" />
                <token id="000000116" type-id="FN_JSON_ARRAY_LENGTH" />
                <token id="000000117" type-id="FN_JSON_EXTRACT" />
                <token id="000000118" type-id="FN_JSON_INSERT" />
                <token id="000000119" type-id="FN_JSON_OBJECT" />
                <token id="000000120" type-id="FN_JSON_REMOVE" />
                <token id="000000121" type-id="FN_JSON_REPLACE" />
                <token id="000000122" type-id="FN_JSON_SET" />
                <token id="000000123" type-id="FN_JSON_TYPE" />
                <token id="000000124" type-id="FN_JSON_VALID" />
                <token id="000000125" type-id="FN_JULIANDAY" />
                <token id="000000126" type-id="FN_LAST_INSERT_ROWID" />
                <token id="000000127" type-id="FN_LENGTH" />
                <token id="000000128" type-id="FN_LIKELIHOOD" />
                <token id="000000129" type-id="FN_LIKELY" />
                <token id="000000130" type-id="FN_LOAD_EXTENSION" />
                <token id="000000131" type-id="FN_LOWER" />
                <token id="000000132" type-id="FN_LTRIM" />
                <token id="000000133" type-id="FN_MAX" />
                <token id="000000134" type-id="FN_MIN" />
                <token id="000000135" type-id="FN_NULLIF" />
                <token id="000000136" type-id="FN_PRINTF" />
                <token id="000000137" type-id="FN_QUOTE" />
                <token id="000000138" type-id="FN_RANDOM" />
                <token id="000000139" type-id="FN_RANDOMBLOB" />
                <token id="000000140" type-id="FN_ROUND" />
                <token id="000000141" type-id="FN_RTRIM" />
                <token id="000000142" type-id="FN_SOUNDEX" />
                <token id="000000143" type-id="FN_SQLITE_COMPILEOPTION_GET" />
                <token id="000000144" type-id="FN_SQLITE_COMPILEOPTION_USED" />
                <token id="000000145" type-id="FN_SQLITE_SOURCE_ID" />
                <token id="000000146" type-id="FN_SQLITE_VERSION" />
                <token id="000000147" type-id="FN_STRFTIME" />
                <token id="000000148" type-id="FN_SUBSTR" />
                <token id="000000149" type-id="FN_SUM" />
                <token id="000000150" type-id="FN_TIME" />
                <token id="000000151" type-id="FN_TOTAL" />
                <token id="000000152" type-id="FN_TOTAL_CHANGES" />
                <token id="000000153" type-id="FN_TRIM" />
                <token id="000000154" type-id="FN_TYPEOF" />
                <token id="000000155" type-id="FN_UNLIKELY" />
                <token id="000000156" type-id="FN_UNICODE" />
                <token id="000000157" type-id="FN_UPPER" />
                <token id="000000158" type-id="FN_ZEROBLOB" />
            </one-of>
            <wrapper id="000000159" template="PARENTHESES">
                <iteration id="000000160" optional="true" separator="CHR_COMMA">
                    <element ref-id="expr" />
                </iteration>
            </wrapper>

        </sequence>
    </element-def>

    <!-- ========= DATA TYPES ========= -->
    <element-def description="Data type" id="data_type">
        <token id="000000161" type-id="DT_BIGINT" />
        <token id="000000162" type-id="DT_BLOB" />
        <token id="000000163" type-id="DT_BOOLEAN" />
        <token id="000000164" type-id="DT_CHARACTER" />
        <token id="000000165" type-id="DT_CLOB" />
        <token id="000000166" type-id="DT_DATE" />
        <token id="000000167" type-id="DT_DATETIME" />
        <token id="000000168" type-id="DT_DECIMAL" />
        <token id="000000169" type-id="DT_DOUBLE" />
        <token id="000000170" type-id="DT_DOUBLE_PRECISION" />
        <token id="000000171" type-id="DT_FLOAT" />
        <token id="000000172" type-id="DT_INT" />
        <token id="000000173" type-id="DT_INT2" />
        <token id="000000174" type-id="DT_INT8" />
        <token id="000000175" type-id="DT_INTEGER" />
        <token id="000000176" type-id="DT_MEDIUMINT" />
        <token id="000000177" type-id="DT_NATIVE_CHARACTER" />
        <token id="000000178" type-id="DT_NCHAR" />
        <token id="000000179" type-id="DT_NUMERIC" />
        <token id="000000180" type-id="DT_NVARCHAR" />
        <token id="000000181" type-id="DT_REAL" />
        <token id="000000182" type-id="DT_SMALLINT" />
        <token id="000000183" type-id="DT_TEXT" />
        <token id="000000184" type-id="DT_TINYINT" />
        <token id="000000185" type-id="DT_UNSIGNED_BIG_INT" />
        <token id="000000186" type-id="DT_VARCHAR" />
        <token id="000000187" type-id="DT_VARYING_CHARACTER" />
    </element-def>

    <!-- ========= OPERATORS ========= -->
    <element-def description="EQUALITY operator" id="equality_operator">
        <one-of id="000000188">
            <sequence id="000000189">
                <token id="000000190" optional="true" type-id="CHR_EXCLAMATION" />
                <token exit="true" id="000000191" type-id="CHR_EQUAL" />
            </sequence>
            <sequence id="000000192">
                <token id="000000193" type-id="CHR_LESS" />
                <token exit="true" id="000000194" type-id="CHR_GREATER" />
            </sequence>
        </one-of>
    </element-def>
    <element-def description="GREATER/LESS operator" id="greater_less_operator">
        <one-of id="000000195">
            <sequence id="000000196">
                <token id="000000197" type-id="CHR_GREATER" />
                <token id="000000198" optional="true" type-id="CHR_EQUAL" />
            </sequence>
            <sequence id="000000199">
                <token id="000000200" type-id="CHR_LESS" />
                <token id="000000201" optional="true" type-id="CHR_EQUAL" />
            </sequence>
        </one-of>
    </element-def>
    <element-def description="CONCATENATION operator" id="concatenation_operator">
        <token id="000000202" type-id="CHR_VERTICAL_BAR" />
        <token id="000000203" type-id="CHR_VERTICAL_BAR" />
    </element-def>

    <!-- ========= MISCELLANEOUS ========= -->
    <element-def description="Number" id="number">
        <one-of id="000000204" optional="true">
            <token id="000000205" type-id="CHR_MINUS" />
            <token id="000000206" type-id="CHR_PLUS" />
        </one-of>
        <one-of id="000000207">
            <token id="000000208" type-id="NUMBER" />
            <token id="000000209" type-id="INTEGER" />
        </one-of>
    </element-def>

    <element-def description="Unknown identifier" id="custom_undefined">
        <token id="000000210" type-id="IDENTIFIER" />
    </element-def>
    <element-def description="Statement end" formatting-spacing-before="NO_SPACE" id="end" truncate-on-execution="true">
        <token id="000000211" type-id="CHR_SEMICOLON" />
    </element-def>
</element-defs>
