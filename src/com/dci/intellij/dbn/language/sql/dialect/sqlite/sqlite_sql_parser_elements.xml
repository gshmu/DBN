<?xml version="1.0" encoding="UTF-8"?>
<!--<!DOCTYPE element-defs SYSTEM "/../../../common/definition/language-parser-elements.dtd">--><element-defs language="SQLITE-SQL" version="6.0">
    <element-def attributes="ROOT, STRUCTURE" description="SQL block" icon="FILE_BLOCK_SQL" id="sql_block">
        <iteration id="000000000">
            <element ref-id="sql_statement" />
        </iteration>
    </element-def>

    <element-def attributes="STRUCTURE, EXECUTABLE, STATEMENT, SCOPE_ISOLATION, GENERIC" description="SQL statement" formatting-indent="ABSOLUTE_NONE" id="sql_statement">
        <one-of id="000000001">
            <element ref-id="alter_statement" />
            <element ref-id="commit_statement" />
            <element ref-id="create_statement" />
            <element ref-id="delete_statement" />
            <element ref-id="drop_statement" />
            <element ref-id="explain_statement" />
            <element ref-id="grant_statement" />
            <element ref-id="insert_statement" />
            <element ref-id="merge_statement" />
            <element ref-id="rollback_statement" />
            <element ref-id="rename_statement" />
            <element ref-id="select_statement" />
            <element ref-id="truncate_statement" />
            <element ref-id="update_statement" />
        </one-of>
        <element optional="true" ref-id="end" />
    </element-def>

    <!-- ========= SELECT ========= -->
    <element-def attributes="DATA_MANIPULATION, QUERY, SCOPE_DEMARCATION" description="Subquery" id="subquery" virtual-object="DATASET">
        <element optional="true" ref-id="with_clause" />

    </element-def>

    <element-def description="WITH clause" id="with_clause">
        <token type-id="KW_WITH" id="000000002" />
        <token optional="true" type-id="KW_RECURSIVE" id="000000003" />
        <iteration id="000000004">
            <element ref-id="common_table_expression" />
        </iteration>
    </element-def>
    <element-def description="Common table expression" id="common_table_expression">
        <alias-def attributes="SUBJECT" id="000000005" original-name="table-name" type="DATASET" />
        <wrapper optional="true" template="PARENTHESES" id="000000006">
            <iteration separator="CHR_COMMA" id="000000007">
                <alias-def id="000000008" original-name="column-name" type="COLUMN" />
            </iteration>
        </wrapper>
        <token type-id="KW_AS" id="000000009" />
        <wrapper template="PARENTHESES" id="000000010">
            <element ref-id="subquery" />
        </wrapper>
    </element-def>

    <!-- ========= EXPRESSIONS ========= -->
    <element-def description="Expression" id="expr">
        <one-of id="000000011">
            <element ref-id="compound_expression" />
            <element ref-id="function_expression" />
            <element ref-id="case_expression" />
            <element ref-id="simple_expression" />
            <element ref-id="exists_expression" />
            <element ref-id="between_expression" />
            <element ref-id="in_expression" />
            <element ref-id="cast_expression" />
            <element ref-id="raise_function" />
            <exec-variable id="000000012" />
        </one-of>
    </element-def>
    <element-def description="Simple expression" id="simple_expression">
        <one-of id="000000013" sortable="true">
            <qualified-identifier id="000000014" separator="CHR_DOT">
                <variant>
                    <object-ref id="000000015" local="true" optional="true" type="SCHEMA" />
                    <object-ref id="000000016" local="true" optional="true" type="DATASET" />
                    <object-ref id="000000017" type="COLUMN" />
                </variant>
                <variant>
                    <object-ref id="000000018" local="true" optional="true" type="SCHEMA" />
                    <object-ref id="000000019" local="true" optional="true" type="DATASET" />
                    <token id="000000020" type-id="CHR_STAR" />
                </variant>
                <variant>
                    <alias-ref id="000000021" type="DATASET" />
                    <object-ref id="000000022" type="COLUMN" />
                </variant>
                <variant>
                    <alias-ref id="000000023" type="DATASET" />
                    <alias-ref id="000000024" type="COLUMN" />
                </variant>
                <variant>
                    <alias-ref id="000000025" type="DATASET" />
                    <token id="000000026" type-id="CHR_STAR" />
                </variant>
            </qualified-identifier>
            <token id="000000027" type-id="CHR_STAR" />
            <element ref-id="literal_value" />
            <element ref-id="number" />
            <token id="000000028" type-id="KW_NULL" />
        </one-of>
    </element-def>
    <element-def description="Compound expression" id="compound_expression">
        <one-of id="000000029">
            <sequence id="000000030">
                <element ref-id="unary_operator" />
                <element ref-id="expr" />
            </sequence>
            <sequence id="000000031">
                <element ref-id="expr" />
                <element ref-id="binary_operator" />
                <element ref-id="expr" />
            </sequence>
            <wrapper id="000000032" template="PARENTHESES">
                <one-of id="000000033">
                    <element ref-id="expr" />
                    <element ref-id="subquery" />
                </one-of>
            </wrapper>
            <element ref-id="expr" />
        </one-of>
        <sequence optional="true" id="000000034">
            <one-of id="000000035">
                <token id="000000036" type-id="KW_ISNULL" />
                <token id="000000037" type-id="KW_NOTNULL" />
                <sequence id="000000038">
                    <token id="000000039" type-id="KW_NOT" />
                    <token id="000000040" type-id="KW_NULL" />
                </sequence>
                <sequence id="000000041">
                    <token id="000000042" type-id="KW_COLLATE" />
                    <token id="000000043" type-id="IDENTIFIER" />
                </sequence>
            </one-of>
        </sequence>
    </element-def>
    <element-def description="CASE expression" id="case_expression">
        <token id="000000044" type-id="KW_CASE" />
        <one-of id="000000045" sortable="true">
            <sequence id="000000046">
                <element ref-id="expr" />
                <iteration exit="true" id="000000047">
                    <sequence id="000000048">
                        <token id="000000049" type-id="KW_WHEN" />
                        <element ref-id="expr" />
                        <token id="000000050" type-id="KW_THEN" />
                        <element ref-id="expr" />
                    </sequence>
                </iteration>
            </sequence>
            <iteration id="000000051">
                <sequence id="000000052">
                    <token id="000000053" type-id="KW_WHEN" />
                    <element ref-id="condition" />
                    <token id="000000054" type-id="KW_THEN" />
                    <element ref-id="expr" />
                </sequence>
            </iteration>
        </one-of>
        <sequence id="000000055" optional="true">
            <token id="000000056" type-id="KW_ELSE" />
            <element ref-id="expr" />
        </sequence>
        <token id="000000057" type-id="KW_END" />
    </element-def>
    <element-def description="EXISTS expression" id="exists_expression">
        <token id="000000058" type-id="KW_EXISTS" />
        <wrapper template="PARENTHESES" id="000000059">
            <element ref-id="select-stmt" />
        </wrapper>
    </element-def>
    <element-def description="BETWEEN expression" id="between_expression">
        <token id="000000060" type-id="KW_BETWEEN" />
        <element ref-id="expr" />
        <token id="000000061" type-id="KW_AND" />
        <element ref-id="expr" />
    </element-def>
    <element-def description="IN expression" id="in_expression">
        <token id="000000062" type-id="KW_IN" />
        <one-of id="000000063">
            <wrapper template="PARENTHESES" id="000000064">
                <one-of id="000000065">
                    <element ref-id="select-stmt" />
                    <iteration separator="CHR_COMMA" id="000000066">
                        <element ref-id="expr" />
                    </iteration>
                </one-of>
            </wrapper>
            <qualified-identifier id="000000067" separator="CHR_DOT">
                <variant>
                    <object-ref id="000000068" local="true" type="SCHEMA" />
                    <object-ref id="000000069" local="true" optional="true" type="DATASET" />
                </variant>
            </qualified-identifier>
        </one-of>
    </element-def>
    <element-def description="RAISE function" id="raise_function">
        <token id="000000070" type-id="KW_RAISE" />
        <wrapper template="PARENTHESES" id="000000071">
            <one-of id="000000072">
                <token id="000000073" type-id="KW_IGNORE" />
                <sequence id="000000074">
                    <one-of id="000000075">
                        <token id="000000076" type-id="KW_ROLLBACK" />
                        <token id="000000077" type-id="KW_ABORT" />
                        <token id="000000078" type-id="KW_FAIL" />
                    </one-of>
                    <token id="000000079" type-id="CHR_COMMA" />
                    <element original-name="error-message" ref-id="expr" />
                </sequence>
            </one-of>
        </wrapper>
    </element-def>
    <element-def description="CAST expression" id="cast_expression">
        <token id="000000080" type-id="KW_CAST" />
        <wrapper template="PARENTHESES" id="000000081">
            <sequence id="000000082">
                <element ref-id="expr" />
                <token id="000000083" type-id="KW_AS" />
                <element ref-id="type_name" />
            </sequence>
        </wrapper>
    </element-def>
    <element-def description="Expression list" id="expression_list">
        <one-of id="000000084" sortable="true">
            <iteration id="000000085" separator="CHR_COMMA">
                <element ref-id="expr" />
            </iteration>
            <wrapper id="000000086" template="PARENTHESES">
                <iteration id="000000087" separator="CHR_COMMA">
                    <element ref-id="expr" />
                </iteration>
            </wrapper>
        </one-of>
    </element-def>

    <!-- ========= FUNCTIONS ========= -->
    <element-def description="Declared function expression" id="declared_function_expression"> <!-- CUSTOM-->
        <qualified-identifier id="000000088" separator="CHR_DOT">
            <variant>
                <object-ref id="000000089" optional="true" type="SCHEMA" />
                <object-ref id="000000090" type="FUNCTION" />
            </variant>
        </qualified-identifier>
        <wrapper attributes="METHOD_PARAMETER_HANDLER" exit="true" id="000000091" template="PARENTHESES">
            <iteration id="000000092" optional="true" separator="CHR_COMMA">
                <element ref-id="expr" />
            </iteration>
        </wrapper>
    </element-def>
    <element-def description="Function expression" id="function_expression">
        <sequence id="000000093">
            <one-of id="000000094">
                <token id="000000095" type-id="FN_AVG" />
                <token id="000000096" type-id="FN_ABS" />
                <token id="000000097" type-id="FN_CHANGES" />
                <token id="000000098" type-id="FN_CHAR" />
                <token id="000000099" type-id="FN_COALESCE" />
                <token id="000000100" type-id="FN_COUNT" />
                <token id="000000101" type-id="FN_GROUP_CONCAT" />
                <token id="000000102" type-id="FN_HEX" />
                <token id="000000103" type-id="FN_IFNULL" />
                <token id="000000104" type-id="FN_INSTR" />
                <token id="000000105" type-id="FN_JSON" />
                <token id="000000106" type-id="FN_JSON_ARRAY" />
                <token id="000000107" type-id="FN_JSON_ARRAY_LENGTH" />
                <token id="000000108" type-id="FN_JSON_EXTRACT" />
                <token id="000000109" type-id="FN_JSON_INSERT" />
                <token id="000000110" type-id="FN_JSON_OBJECT" />
                <token id="000000111" type-id="FN_JSON_REMOVE" />
                <token id="000000112" type-id="FN_JSON_REPLACE" />
                <token id="000000113" type-id="FN_JSON_SET" />
                <token id="000000114" type-id="FN_JSON_TYPE" />
                <token id="000000115" type-id="FN_JSON_VALID" />
                <token id="000000116" type-id="FN_JULIANDAY" />
                <token id="000000117" type-id="FN_LAST_INSERT_ROWID" />
                <token id="000000118" type-id="FN_LENGTH" />
                <token id="000000119" type-id="FN_LIKELIHOOD" />
                <token id="000000120" type-id="FN_LIKELY" />
                <token id="000000121" type-id="FN_LOAD_EXTENSION" />
                <token id="000000122" type-id="FN_LOWER" />
                <token id="000000123" type-id="FN_LTRIM" />
                <token id="000000124" type-id="FN_MAX" />
                <token id="000000125" type-id="FN_MIN" />
                <token id="000000126" type-id="FN_NULLIF" />
                <token id="000000127" type-id="FN_PRINTF" />
                <token id="000000128" type-id="FN_QUOTE" />
                <token id="000000129" type-id="FN_RANDOM" />
                <token id="000000130" type-id="FN_RANDOMBLOB" />
                <token id="000000131" type-id="FN_ROUND" />
                <token id="000000132" type-id="FN_RTRIM" />
                <token id="000000133" type-id="FN_SOUNDEX" />
                <token id="000000134" type-id="FN_SQLITE_COMPILEOPTION_GET" />
                <token id="000000135" type-id="FN_SQLITE_COMPILEOPTION_USED" />
                <token id="000000136" type-id="FN_SQLITE_SOURCE_ID" />
                <token id="000000137" type-id="FN_SQLITE_VERSION" />
                <token id="000000138" type-id="FN_STRFTIME" />
                <token id="000000139" type-id="FN_SUBSTR" />
                <token id="000000140" type-id="FN_SUM" />
                <token id="000000141" type-id="FN_TIME" />
                <token id="000000142" type-id="FN_TOTAL" />
                <token id="000000143" type-id="FN_TOTAL_CHANGES" />
                <token id="000000144" type-id="FN_TRIM" />
                <token id="000000145" type-id="FN_TYPEOF" />
                <token id="000000146" type-id="FN_UNLIKELY" />
                <token id="000000147" type-id="FN_UNICODE" />
                <token id="000000148" type-id="FN_UPPER" />
                <token id="000000149" type-id="FN_ZEROBLOB" />
            </one-of>
            <wrapper id="000000150" optional="true" template="PARENTHESES">
                <iteration id="000000151" optional="true" separator="CHR_COMMA">
                    <element ref-id="expr" />
                </iteration>
            </wrapper>

        </sequence>
    </element-def>

    <!-- ========= DATA TYPES ========= -->
    <element-def description="Data type" id="data_type">
        <token id="000000152" type-id="DT_BIGINT" />
        <token id="000000153" type-id="DT_BLOB" />
        <token id="000000154" type-id="DT_BOOLEAN" />
        <token id="000000155" type-id="DT_CHARACTER" />
        <token id="000000156" type-id="DT_CLOB" />
        <token id="000000157" type-id="DT_DATE" />
        <token id="000000158" type-id="DT_DATETIME" />
        <token id="000000159" type-id="DT_DECIMAL" />
        <token id="000000160" type-id="DT_DOUBLE" />
        <token id="000000161" type-id="DT_DOUBLE_PRECISION" />
        <token id="000000162" type-id="DT_FLOAT" />
        <token id="000000163" type-id="DT_INT" />
        <token id="000000164" type-id="DT_INT2" />
        <token id="000000165" type-id="DT_INT8" />
        <token id="000000166" type-id="DT_INTEGER" />
        <token id="000000167" type-id="DT_MEDIUMINT" />
        <token id="000000168" type-id="DT_NATIVE_CHARACTER" />
        <token id="000000169" type-id="DT_NCHAR" />
        <token id="000000170" type-id="DT_NUMERIC" />
        <token id="000000171" type-id="DT_NVARCHAR" />
        <token id="000000172" type-id="DT_REAL" />
        <token id="000000173" type-id="DT_SMALLINT" />
        <token id="000000174" type-id="DT_TEXT" />
        <token id="000000175" type-id="DT_TINYINT" />
        <token id="000000176" type-id="DT_UNSIGNED_BIG_INT" />
        <token id="000000177" type-id="DT_VARCHAR" />
        <token id="000000178" type-id="DT_VARYING_CHARACTER" />
    </element-def>

    <element-def description="Type name" id="type_name">
        <iteration id="000000179">
            <element ref-id="data_type" />
        </iteration>
        <wrapper optional="true" template="PARENTHESES" id="000000180">
            <sequence id="000000181">
                <element original-name="signed-number" ref-id="number" />
                <sequence optional="true" id="000000182">
                    <token type-id="CHR_COMMA" id="000000183" />
                    <element original-name="signed-number" ref-id="number" />
                </sequence>
            </sequence>
        </wrapper>
    </element-def>

    <!-- ========= OPERATORS ========= -->
    <element-def description="UNARY operator" id="unary_operator">
        <token id="000000184" type-id="CHR_PLUS" />
        <token id="000000185" type-id="CHR_MINUS" />
        <token id="000000186" type-id="CHR_TILDE" />
        <token id="000000187" type-id="KW_NOT" />
    </element-def>
    <element-def description="BINARY operator" id="binary_operator">
        <one-of id="000000188">
            <token id="000000189" type-id="CHR_STAR" />
            <token id="000000190" type-id="CHR_SLASH" />
            <token id="000000191" type-id="CHR_PERCENT" />
            <token id="000000192" type-id="CHR_PLUS" />
            <token id="000000193" type-id="CHR_MINUS" />
            <token id="000000194" type-id="KW_AND" />
            <token id="000000195" type-id="KW_OR" />
            <token id="000000196" type-id="KW_IN" />
            <sequence id="000000197">
                <token id="000000198" optional="true" type-id="KW_NOT" />
                <one-of id="000000199">
                    <token id="000000200" type-id="KW_LIKE" />
                    <token id="000000201" type-id="KW_GLOB" />
                    <token id="000000202" type-id="KW_MATCH" />
                    <token id="000000203" type-id="KW_REGEXP" />
                </one-of>
            </sequence>
            <token id="000000204" type-id="KW_COLLATE" />
            <sequence id="000000205">
                <token id="000000206" type-id="KW_IS" />
                <token id="000000207" optional="true" type-id="KW_NOT" />
            </sequence>
            <sequence id="000000208">
                <token id="000000209" type-id="CHR_VERTICAL_BAR" />
                <token id="000000210" optional="true" type-id="CHR_VERTICAL_BAR" />
            </sequence>
            <sequence id="000000211">
                <token id="000000212" type-id="CHR_EXCLAMATION" />
                <token id="000000213" type-id="CHR_EQUAL" />
            </sequence>
            <sequence id="000000214">
                <token id="000000215" type-id="CHR_EQUAL" />
                <token id="000000216" optional="true" type-id="CHR_EQUAL" />
            </sequence>
            <sequence id="000000217">
                <token id="000000218" type-id="CHR_LESS" />
                <one-of optional="true" id="000000219">
                    <token id="000000220" type-id="CHR_LESS" />
                    <token id="000000221" type-id="CHR_EQUAL" />
                    <token id="000000222" type-id="CHR_GREATER" />
                </one-of>
            </sequence>
            <sequence id="000000223">
                <token id="000000224" type-id="CHR_GREATER" />
                <one-of optional="true" id="000000225">
                    <token id="000000226" type-id="CHR_LESS" />
                    <token id="000000227" type-id="CHR_EQUAL" />
                    <token id="000000228" type-id="CHR_GREATER" />
                </one-of>
            </sequence>
        </one-of>
    </element-def>

    <element-def description="Literal value" id="literal_value">
        <token id="000000229" type-id="STRING" />
        <token id="000000230" type-id="KW_NULL" />
        <token id="000000231" type-id="KW_CURRENT_TIME" />
        <token id="000000232" type-id="KW_CURRENT_DATE" />
        <token id="000000233" type-id="KW_CURRENT_TIMESTAMP" />
    </element-def>


    <!-- ========= MISCELLANEOUS ========= -->
    <element-def description="Number" id="number">
        <one-of id="000000234" optional="true">
            <token id="000000235" type-id="CHR_MINUS" />
            <token id="000000236" type-id="CHR_PLUS" />
        </one-of>
        <one-of id="000000237">
            <token id="000000238" type-id="NUMBER" />
            <token id="000000239" type-id="INTEGER" />
        </one-of>
    </element-def>

    <element-def description="Unknown identifier" id="custom_undefined">
        <token id="000000240" type-id="IDENTIFIER" />
    </element-def>
    <element-def description="Statement end" formatting-spacing-before="NO_SPACE" id="end" truncate-on-execution="true">
        <token id="000000241" type-id="CHR_SEMICOLON" />
    </element-def>
</element-defs>
