<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.dbn.oracleAI.ui.OracleAIChatBox">
  <grid id="27dc6" binding="chatBoxMainPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="728" height="667"/>
    </constraints>
    <properties>
      <opaque value="true"/>
      <preferredSize width="-1" height="-1"/>
    </properties>
    <border type="empty">
      <size top="5" left="5" bottom="5" right="5"/>
    </border>
    <children>
      <grid id="5a9ba" binding="companionCommandPanel" layout-manager="GridLayoutManager" row-count="3" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="135be" class="javax.swing.JComboBox" binding="AIModelComboBox">
            <constraints>
              <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="7" hsize-policy="7" anchor="8" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <model>
                <item value="command"/>
                <item value="command-nightly"/>
                <item value="command-light"/>
              </model>
              <toolTipText resource-bundle="Messages" key="companion.chat.model.tooltip"/>
            </properties>
          </component>
          <component id="e55fc" class="javax.swing.JComboBox" binding="profileComboBox">
            <constraints>
              <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="5" hsize-policy="5" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <editable value="false"/>
              <toolTipText resource-bundle="Messages" key="companion.chat.profile.tooltip"/>
            </properties>
          </component>
          <vspacer id="8cce3">
            <constraints>
              <grid row="2" column="0" row-span="1" col-span="2" vsize-policy="4" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false">
                <minimum-size width="-1" height="10"/>
              </grid>
            </constraints>
          </vspacer>
          <component id="e82ed" class="javax.swing.JProgressBar" binding="activityProgress">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="2" vsize-policy="1" hsize-policy="0" anchor="1" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <enabled value="true"/>
              <focusCycleRoot value="false"/>
              <indeterminate value="true"/>
              <maximum value="100"/>
              <visible value="true"/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="709e" binding="companionConversationPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties>
          <autoscrolls value="false"/>
        </properties>
        <border type="none">
          <size top="10" left="20" bottom="10" right="20"/>
        </border>
        <children>
          <grid id="4f983" binding="companionConversationPanelTop" layout-manager="GridLayoutManager" row-count="1" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
            <margin top="0" left="0" bottom="0" right="0"/>
            <constraints border-constraint="North"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="9c819" class="javax.swing.JCheckBox" binding="explainSQLCheckbox">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <text resource-bundle="Messages" key="companion.explainsql.checkbox"/>
                  <toolTipText resource-bundle="Messages" key="companion.explainsql.tooltip"/>
                </properties>
              </component>
            </children>
          </grid>
          <scrollpane id="c0d7e" binding="companionConversationPan">
            <constraints border-constraint="Center"/>
            <properties/>
            <border type="none"/>
            <children/>
          </scrollpane>
        </children>
      </grid>
      <component id="bb839" class="javax.swing.JTextField" binding="chatBoxNotificationMessage">
        <constraints border-constraint="South"/>
        <properties>
          <editable value="false"/>
          <text value=""/>
        </properties>
      </component>
    </children>
  </grid>
</form>
